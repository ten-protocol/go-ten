{
  "exe": "main",
  "key": "testnet.pem",
  "debug": true,
  "heapSize": 4096,
  "executableHeap": true,
  "productID": 1,
  "securityVersion": 1,
  "mounts": [
    {
      "source": "/data",
      "target": "/data",
      "type": "hostfs",
      "readOnly": false
    }
  ],
  "env": [
    {
      "name": "TESTMODE",
      "value": "false"
    }
  ],
  "files": [
    {
      "source": "/etc/ssl/certs/ca-certificates.crt",
      "target": "/etc/ssl/certs/ca-certificates.crt"
    }
  ]
}